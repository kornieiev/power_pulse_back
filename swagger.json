          }
        ],
        "security": [],
        "responses": {
          "200": {
            "description": "Verification successful",
            "content": {}
          },
          "404": {
            "description": "User not found",
            "content": {}
          }
        }
      }
    },

    "/users/login": {
      "post": {
        "tags": ["Authorization"],
        "summary": "User login",
        "parameters": [],
        "security": [],
